/* pages/Personal/login/login.wxss */
page {
    width: 100%;
    height: 100%;
}

.login {
    width: 100%;
    height: 80%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
    box-sizing: border-box;
}

.register {
    display: flex;
    align-items: center;
    justify-content: center;
    margin-top: 50px;
}
